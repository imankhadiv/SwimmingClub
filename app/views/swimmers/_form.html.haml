= form_for @swimmer do |f|
  - if @swimmer.errors.any?
    #error_explanation
      %h2= "#{pluralize(@swimmer.errors.count, "error")} prohibited this swimmer from being saved:"
      %ul
        - @swimmer.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :asa_number
    = f.text_field :asa_number
  .field
    = f.label :asa_category
    = f.text_field :asa_category
  .field
    = f.label :date_of_birth
    = f.date_select :date_of_birth
  .field
    = f.label :sex
    = f.text_field :sex
  .field
    = f.label :photo_consent
    = f.text_field :photo_consent
  .field
    = f.label :current_squad
    = f.text_field :current_squad
  .field
    = f.label :user
    = f.text_field :user
  .actions
    = f.submit 'Save'


