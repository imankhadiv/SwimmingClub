-#- females = SwimmerTime.best_times 'Female'
-#- males = SwimmerTime.best_times 'Male'

#best_timess

  -#.alert.alert-success
  -#  %h2 Best Club Times
  -#%br

  .row
    .col-md-10.col-md-offset-1
      .alert.alert-warning.text-center
        %h3 Girls
        -@females.keys.each do |item|
          .alert.alert-info.text-center
            %caption= " #{item.first} STROKE\t #{item.second} Meters\t #{item.third} year-olds"
          %table.table-bordered.table-condensed.table.table-striped.table-responsive.table-customstyle
            %thead
              %th Name
              %th Time
              %th Meet
              %th Venue
              %th Course
              -#%th Age
              %th Date
            %tbody
              %td= link_to @females[item].first.swimmer.swimmer_name, swimmer_times_swimmer_path(@females[item].first.swimmer_id)
              %td= @females[item].first.convert_times
              %td= @females[item].first.meet
              %td= @females[item].first.venue
              %td= @females[item].first.course
              -#%td= @females[item].first.age
              %td= @females[item].first.date
  .row
    .col-md-10.col-md-offset-1
      .alert.alert-warning.text-center
        %h3 Boys
        -@males.keys.each do |item|
          .alert.alert-info.text-center
            %caption= " #{item.first} STROKE\t #{item.second} Meters\t #{item.third} year-olds"
          %table.table-bordered.table-condensed.table.table-striped.table-responsive.table-customstyle
            %thead
              %th Name
              %th Time
              %th Meet
              %th Venue
              %th Course
              -#%th Age
              %th Date
            %tbody
              %td= link_to @males[item].first.swimmer.swimmer_name, swimmer_times_swimmer_path(@males[item].first.swimmer_id)
              %td= @males[item].first.convert_times
              %td= @males[item].first.meet
              %td= @males[item].first.venue
              %td= @males[item].first.course
              -#%td= @males[item].first.age
              %td= @males[item].first.date

